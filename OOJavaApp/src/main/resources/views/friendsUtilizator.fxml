<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPanelMainBackground" prefHeight="477.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.controller.PrieteniiController">

    <children>
        <Button fx:id="addFriendButton" layoutX="449.0" layoutY="325.0" mnemonicParsing="false" onAction="#handleAddFriends" prefHeight="16.0" prefWidth="96.0" text="Send Request" />
        <Button fx:id="deleteFriendButton" layoutX="143.0" layoutY="325.0" mnemonicParsing="false" onAction="#handleDeleteFriends" prefHeight="16.0" prefWidth="96.0" text="Delete Friend" />
        <TableView fx:id="friendsTable" layoutX="67.0" layoutY="43.0" prefHeight="272.0" prefWidth="248.0">
            <columns>
                <TableColumn fx:id="columnFriendFirstName" prefWidth="78.4000015258789" text="First Name" />
                <TableColumn fx:id="columnFriendLastName" prefWidth="82.79998779296875" text="Last Name" />
            <TableColumn fx:id="columnFriendDate" prefWidth="84.79998779296875" text="Date" />
            </columns>
        </TableView>
        <TableView fx:id="usersTable" layoutX="393.0" layoutY="43.0" prefHeight="272.0" prefWidth="242.0">
            <columns>
                <TableColumn fx:id="columnUserID" prefWidth="63.0" text="User ID" />
                <TableColumn fx:id="columnUserFirstName" prefWidth="83.0" text="First Name" />
                <TableColumn fx:id="columnUserLastName" prefWidth="100.0" text="Last Name" />
            </columns>
        </TableView>
        <Label fx:id="labelFriends" layoutX="68.0" layoutY="28.0" text="Friends">
         <font>
            <Font name="MS Outlook" size="12.0" />
         </font></Label>
        <Label fx:id="labelAllUsers" layoutX="392.0" layoutY="28.0" text="All Users">
         <font>
            <Font name="MS Outlook" size="12.0" />
         </font></Label>
      <Button fx:id="showRequestsButton" layoutX="267.0" layoutY="324.0" mnemonicParsing="false" onAction="#handleShowRequests" prefHeight="26.0" prefWidth="157.0" text="Show Received Requests" />
      <Button fx:id="showSentReqButton" layoutX="267.0" layoutY="370.0" mnemonicParsing="false" onAction="#handleShowSentReq" prefHeight="26.0" prefWidth="157.0" text="Show Sent Requests" />
      <Button fx:id="buttonTelegram" layoutX="107.0" layoutY="424.0" mnemonicParsing="false" onAction="#handleShowTelegram" prefHeight="26.0" prefWidth="168.0" text="Telegram" />
      <Button fx:id="conversationButton" layoutX="440.0" layoutY="424.0" mnemonicParsing="false" onAction="#handleConversation" prefHeight="26.0" prefWidth="147.0" text="Show Conversation" />
      <Label fx:id="userLabel" layoutX="580.0" layoutY="7.0" prefHeight="14.0" prefWidth="96.0" textFill="#a716a4">
         <font>
            <Font name="Californian FB" size="12.0" />
         </font></Label>
      <Label fx:id="labelLogged" layoutX="514.0" layoutY="6.0" text="Logged in as: " textFill="#a716a4">
         <font>
            <Font name="Californian FB" size="12.0" />
         </font>
      </Label>
      <Button fx:id="buttonBack" mnemonicParsing="false" onAction="#handleBack" text="&lt;" />
      <Button fx:id="buttonDate" layoutX="320.0" layoutY="425.0" mnemonicParsing="false" onAction="#handleDate" text="Select a date" />
    </children>
</AnchorPane>
